number1  = input('Enter a number: ')
num1 = int(number1)
sign = input('Enter the sign: ')
number2 = input('Enter another number: ')
num2 = int(number2)
if sign == '+':
    print(num1 + num2)
elif sign == '-':
    print(num1 - num2)
elif sign == '*':
    print(num1 * num2)
elif sign == "/":
    print(num1 / num2)
